<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefHeight="664.0" prefWidth="431.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.projektjava.LoginController">

    <VBox alignment="CENTER" layoutX="70.0" layoutY="210.0" prefHeight="244.0" prefWidth="291.0" spacing="10">

        <Label style="-fx-font-size: 18pt;" text="Sign Up" />

        <Label fx:id="successLabel" style="-fx-text-fill: green;" text="" />

        <TextField fx:id="SUusernameField" promptText="Username" />

        <PasswordField fx:id="SUpasswordField" promptText="Password" />

        <Button onAction="#signUpButtonAction" text="Sign Up" />

        <Button onAction="#backToLoginButtonAction" text="Back to Login" />

    </VBox>

</AnchorPane>
